Item }=useCart();\n    return(\n        <>\n        <div className=\"col-11 col-md-6 col-lg-3 mx-0 mb-4\">\n        <div class=\"card p-0 overflow-hidden h-100 shadow\">\n  <img src={props.img} class=\"card-img-top img-fluid\" />\n  <div className=\"card-body text-center\">\n    <h5 className=\"card-title\">{props.title}</h5>\n    <h5 className=\"card-title\">$ {props.price}</h5>\n    <p className=\"card-text\">{props.desc}</p>\n    <button className=\"btn btn-success\" onClick={()=>addItem(props.item)}>Add To Cart</button>\n  </div>\n</div>\n        </div>\n        </>\n    )\n}\n\nexport default CarItem;\n","/Users/mayank.agrawal/Desktop/react-app/Shopping_Cart/src/data.js",["41"],"import react from 'react'\nimport img1 from './image/car_1.png'\nimport img2 from './image/car_2.png'\nimport img3 from './image/car_3.png'\nimport img4 from './image/car_4.png'\nimport img5 from './image/car_5.png'\nimport img6 from './image/car_6.png'\n\nconst data={\n    productData:[\n        {\n            id:1,\n            img:img1,\n            title:'this is car 1',\n            desc:'',\n            price:5000\n            \n        },\n        {\n            id:2,\n            img:img2,\n            title:'this is car 2',\n            desc:'',\n            price:1000\n            \n        },\n        {\n            id:3,\n            img:img3,\n            title:'this is car 3',\n            desc:'',\n            price:1000\n            \n        },\n        {\n            id:4,\n            img:img4,\n            title:'this is car 4',\n            desc:'',\n            price:1000\n            \n        },\n        {\n            id:5,\n            img:img5,\n            title:'this is car 5',\n            desc:'',\n            price:1000\n            \n        },\n        {\n            id:6,\n            img:img6,\n            title:'this is car 6',\n            desc:'',\n            price:1000\n            \n        }\n    ],\n}\n\nexport default data;",{"ruleId":"42","s